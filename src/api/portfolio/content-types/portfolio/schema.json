{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": "MarqIT Portfolio Projects - Homepage and Portfolio Page"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tech": {
      "type": "json"
    },
    "category": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "icon": {
      "type": "string",
      "default": "Monitor"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "showOnHomepage": {
      "type": "boolean",
      "default": true
    },
    "statistics": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "order": {
      "type": "integer"
    },
    "liveUrl": {
      "type": "string"
    },
    "githubUrl": {
      "type": "string"
    },
    "caseStudyUrl": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
